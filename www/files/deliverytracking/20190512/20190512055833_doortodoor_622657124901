			f.fsp_action.value = "PARC_ACT_002";
			f.fsp_cmd.value = "retrieveInchgSaleSmlInfoACT";
			f.nextpage.value = "popup/pop_salesMap.jsp";
			f.target = "wbOld";
			f.submit();
		}
